<%= javascript_include_tag  'unapprove_installation_name'%>
<div class="approve_installation_name_content">
	<%unless @validinstallationname.blank? %>
		<span class='unapprove_loader loader_spin'><%= image_tag("/images/big-spinner.gif") %></span>
		<div class="padding_top_5_space"></div>
		<div class="paddingleft_15 text_font"><label>Unapprover : </label></div>
		<div class="padding_top_5_space"></div>
		<div class="paddingleft_15"><%= text_field_tag "dig_installation_unapprover_name", @approver, :class =>"contentCSPsel_ins_approve", :disabled => false, :html=>{:style => "text-transform:capitalize"}%></div>
		<div class="padding_top_5_space"></div>
		<div class="message_valid" ><span id="dig_in_crc_unapprove"></span></div>
		<input type="hidden" id="validinstallationname" name="validinstallationname" value = "<%= @validinstallationname %>" />
		<div class="padding_left_15_margin_5">
		    <button id="unapprove_installation" class='unapprove_installation width_70'>Ok</button>
			<button id="unapprove_installation_cancel" class='cancel_unapprove width_70'>Cancel</button>
		</div>
	<%else%>
		<div>Please select installation name and try again.</div>
	<%end%>
</div>
