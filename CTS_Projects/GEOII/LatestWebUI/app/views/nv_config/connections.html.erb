<%= javascript_include_tag 'module_connections' %>
<div id="site_content">
	<div class='left_content'>
	  <h3 class='sub_header'>Modules</h3> 
	  <% @modules.each_with_index do |mod, i| %>
	    <p>
	      <%if params[:channel] == i.to_s  
	        cls="menu_selected" 
	      end%>
	      <%=link_to "#{mod.group_name}", {:action=>:feeder, :channel =>i}, :class=>"menu_link_item #{cls}"%> 
	    </p>
	  <% end %>
	</div>
	<div class="right_content" id="module_content">
		<%= GenericHelper.toolbar({ 'unlock' => 'unlock.png', 'nv_save' => 'savemouseover.png', 'nv_refresh' => 'refresh_mouse_over.png', 'nv_default' => 'default.png'})%>
		<div class="nv_config_channels_content_wrapper">
			<%=render :partial=> "parameters"%>
		</div>
	</div>
</div>