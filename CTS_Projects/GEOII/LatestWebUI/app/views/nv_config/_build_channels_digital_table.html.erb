<% 
	index = 0 
	if io_assignment
		arrow_class = 'nv_io_assignment_arrow'
	else
		arrow_class = 'nv_arrow'
	end
%>
<% if !@rebuild %>
<div class="channels_selection">
<% end %>
	<div class="channels_selection_table_wrapper_digital">
		<table>
		<% @channels.each do |channel| 
		 	if @current_channel == channel[:Group_Channel] 
		 		td_class = ' selected_row' 
		 	else
		 		td_class = ''
		 	end 
		%>
			<tr class="<%=(index % 2 == 0 ? 'light' : 'dark')%>_row">
			    <td class="nv_channel<%=td_class%>"><%= channel[:Group_Channel] %></td>
				<td class="nv_name_digital<%=td_class%>"><%= @channel_labels[channel[:Group_Channel]] %>
				<td class="nv_name<%=td_class%>"><%= channel[:String] %></td>
				<td class="<%=arrow_class%><%=td_class%>"><img alt="Arrow" border="0" src="/images/ladder_logic/arrow.png"/></td>
			</tr>
			<% index += 1 %>
		<% end %>
		<% (index..@number_per_page-1).each do |i|%>
			<tr class="<%=(index % 2 == 0 ? 'light' : 'dark')%>_row"><td></td><td></td><td></td></tr>
			<% index += 1 %>
		<% end %>
		</table>
	</div>
<% if !@rebuild %>
</div>
<% end %>
