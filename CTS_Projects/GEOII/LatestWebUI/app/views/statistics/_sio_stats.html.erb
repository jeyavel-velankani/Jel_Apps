<table cellspacing="0" cellpadding="0" border="0" class="statstable" width="100%" class="logtable" style="color:#454545;">
  <thead>
    <tr>
      <th class="title" width="150"><span style="color:#CFD631;font-size:12px;">SIO Statistics</span></th>
      <th class="title" width="100"><span style="color:#CFD631;font-size:12px;">Value</span></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td class="txtleft"><span style="color:#fff">Bad SIO Packets</span></td>
        <td class="txtleft"><span style="color:#fff"><%= @currentrequest.bad_sio_pkt_cnt %></span></td>
      </tr>
      <tr>
        <td class="txtleft"><span style="color:#fff">SIO Tx Packets</span></td>
        <td class="txtleft"><span style="color:#fff"><%= @currentrequest.sio_tx_pkt_cnt %></span></td>
      </tr>
      <tr>
        <td class="txtleft"><span style="color:#fff">SIO Rx Packets</span></td>
        <td class="txtleft"><span style="color:#fff"><%= @currentrequest.sio_rx_pkt_cnt %></span></td>
      </tr>
      <tr>
        <td class="txtleft"><span style="color:#fff">SPI Tx Q Full Count</span></td>
        <td class="txtleft"><span style="color:#fff"><%= @currentrequest.spi_tx_q_full_cnt %></span></td>
      </tr>
      <tr>
        <td class="txtleft"><span style="color:#fff">SPI Rx Q Full Count</span></td>
        <td class="txtleft"><span style="color:#fff"><%= @currentrequest.spi_rx_q_full_cnt %></span></td>
      </tr>
  </tbody>
</table>

<script type="text/javascript">
  $('.statstable').flexigrid({width: "90%", height:'auto',striped:false});
</script>