<% 	track_parameters = get_track_parameters(channel_parameters, "3.VTI", channel_index) %>
<div class="status_data">
	<div class="outer_box" style='margin:0;border-bottom: none;'>
<div id="titles" class="title">
    <span><%#= channel_name(channel_index, atcs_address, card_index, "VTI") %></span>
</div>	
<div>
    <span id="titles" style='padding-left:8px;'>Rx</span>
    <span id="titles" class="sub_title">
    	<% if channel_parameters["3.VTI#{channel_index}.VitalCodePresent"] == 1 %>
			VCP
		<% elsif channel_parameters["3.VTI#{channel_index}.CodePresent"] == 1  %>
			CP
		<% else %>
			NC	
		<% end %>
	</span>
</div>
<table cellspacing=0 cellpadding=0 border=1 id="coded_track_vars">
    <tr>
		<td id="coded_track_table_data" class='nobackground_table'><%= track_parameters[:track1] %></td>
        <td id="coded_track_table_data" class='nobackground_table'><%= track_parameters[:track2] %></td>
    </tr>
    <tr>
		<td id="coded_track_table_data" class='nobackground_table'><%= track_parameters[:track3] %></td>
		<td id="coded_track_table_data" class='nobackground_table'><%= track_parameters[:track4] %></td>
    </tr>
    <tr>
        <td id="coded_track_table_data" colspan=2 class='nobackground_table'>
        	<% if card_type.to_i == 4 %>
				<%  if(@scaled_parameters["#{card_index}.3.LIN_ANLG1.VTICurrent"].blank?) %>
				      <%= channel_parameters["3.LIN_ANLG1.VTICurrent"].blank? ? "0" : ( '%g' % sprintf('%.4f', (channel_parameters["3.LIN_ANLG1.VTICurrent"].to_f * 0.01))) %> A
				<%  else %>
				      <%= channel_parameters["3.LIN_ANLG1.VTICurrent"].blank? ? "0" : ( '%g' % sprintf('%.4f', ((channel_parameters["3.LIN_ANLG1.VTICurrent"].to_f * @scaled_parameters["#{card_index}.3.LIN_ANLG1.VTICurrent"].to_f/1000)/1000))) %> A 
				<%  end %>
			<% else %>
				<%  if(@scaled_parameters["#{card_index}.3.TRK_ANLG_1.VTICurrent"].blank?) %>
				      <%= channel_parameters["3.TRK_ANLG_1.VTICurrent"].blank? ? "0" : ( '%g' % sprintf('%.4f', (channel_parameters["3.TRK_ANLG_1.VTICurrent"].to_f * 0.01))) %> A
				<%  else %>
				      <%= channel_parameters["3.TRK_ANLG_1.VTICurrent"].blank? ? "0" : ( '%g' % sprintf('%.4f', ((channel_parameters["3.TRK_ANLG_1.VTICurrent"].to_f * @scaled_parameters["#{card_index}.3.TRK_ANLG_1.VTICurrent"].to_f/1000)/1000))) %> A 
				<%  end %>
			<% end %>
			
            <%#= vti_current.blank? ? "0.0 A" : "#{vti_current.current_value.to_f * 0.01} A" %>
        </td>
    </tr>
</table>
</div>
</div>
