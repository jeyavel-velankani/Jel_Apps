<%
	tx_current = track["data"][0]['Code']['vco']['track_values']["current"]
	tx_voltage = track["data"][0]['Code']['vco']['track_values']["voltage"]
	tx_1 = track["data"][0]['Code']['vco']['track_params']["track1"]
	tx_2 = track["data"][0]['Code']['vco']['track_params']["track2"]
	tx_3 = track["data"][0]['Code']['vco']['track_params']["track3"]
	tx_4 = track["data"][0]['Code']['vco']['track_params']["track4"]
	rx_current = track["data"][0]['Code']['Vti']['current'] 
	rx_1 = track["data"][0]['Code']['Vti']['track_params']["track1"]
	rx_2 = track["data"][0]['Code']['Vti']['track_params']["track2"]
	rx_3 = track["data"][0]['Code']['Vti']['track_params']["track3"]
	rx_4 = track["data"][0]['Code']['Vti']['track_params']["track4"]

%>
<div class="track_table">
	<table>
		<tr><td>Tx</td><td class="tx_params"><table><tr><td><%=tx_1%></td><td><%=tx_2%></td><td><%=tx_3%></td><td><%=tx_4%></td></tr></table></td></tr>
		<tr class="even"><td>Tx Voltage</td><td class="tx_voltage_value"><%=tx_voltage.to_s%></td></tr>
		<tr><td>Tx Current</td><td class="tx_current_value"><%=tx_current.to_s%></td></tr>
		<tr class="even"><td>Rx</td><td class="rx_params"><table><tr><td><%=rx_1%></td><td><%=rx_2%></td><td><%=rx_3%></td><td><%=rx_4%></td></tr></table></td></tr>
		<tr><td>Rx Current</td><td class="rx_current_value"><%=rx_current.to_s%></td></tr>
	</table>
</div>