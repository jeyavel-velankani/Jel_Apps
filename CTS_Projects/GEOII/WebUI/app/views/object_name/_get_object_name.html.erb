<%= javascript_include_tag  "object_name"%>
<%= stylesheet_link_tag 'v_config.css'%>

<div id="object_container">
	<div class="v_config_wrapper">
		<%= GenericHelper.toolbar({ 'v_save' => 'savemouseover.png', 'v_refresh' => 'refresh_mouse_over.png', 'default_object' => 'default.png'})%>
		<div class="message_container">&nbsp;<span class="success_message"></span></div>
		<div id="object_name_container">
			<%= render :partial => "object_name_settings"%>
		</div>	
		<%= hidden_field_tag 'object_type_name', params[:object_type_name] %>
		<%= hidden_field_tag 'name_type', params[:name_type] %>	
	</div>
</div>
